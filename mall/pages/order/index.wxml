<Tabs 
  tabs="{{tabs}}"
  bindtabsItemChange="handleTabsItemChange"
  >
    <block>
      <view class="first_tab">
        <view class="order_item" wx:for="{{orders}}" wx:key="order_id">
          <view class="order_num">
            <text>订单编号</text>
            <text>{{item.order_number}}</text>
          </view>
          <view class="order_price">
            <text>订单价格</text>
            <text>￥{{item.order_price}}</text>
          </view>
          <view class="order_date">
            <text>订单日期</text>
            <text>{{item.time}}</text>
          </view>
        </view>
      </view>
    </block>
  </Tabs>